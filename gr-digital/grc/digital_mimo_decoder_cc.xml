<?xml version="1.0"?>
<block>
  <name>MIMO Decoder</name>
  <key>digital_mimo_decoder_cc</key>
  <category>[MIMO]</category>
  <import>from gnuradio import digital</import>
  <make>digital.mimo_decoder_cc($N, $mimo_technique, $vlen)</make>
  <param>
    <name>N</name>
    <key>N</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>MIMO technique</name>
    <key>mimo_technique</key>
    <value>vblast</value>
    <type>string</type>
    <option>
      <name>V-BLAST</name>
      <key>vblast</key> 
    </option>
    <option>
      <name>Alamouti</name>
      <key>alamouti</key>
    </option>
    <option>
      <name>Differential Alamouti</name>
      <key>diff_stbc</key>
    </option>
  </param>
  <param>
    <name>Vector Length</name>
    <key>vlen</key>
    <value>1</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <nports>$N</nports>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
